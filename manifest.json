{
  "manifest_version": 3,
  "name": "Countdown Timer",
  "version": "1.0",
  "permissions": ["tabs", "notifications", "storage", "windows"],
  "action": {
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    },
    "default_title": "Countdown Timer"
  },
  "background": {
    "service_worker": "background.js"
  }
}
